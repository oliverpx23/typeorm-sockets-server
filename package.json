{
  "name": "turnerodigitalbackend",
  "version": "1.0.0",
  "description": "backend REST + Sockets",
  "main": "server.ts",
  "dependencies": {
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "socket.io": "^2.3.0",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.0.8",
    "@types/socket.io": "^2.1.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [
    "backend"
  ],
  "author": "Oliver Pots",
  "license": "ISC"
